<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
    <xsd:complexType 
        name="PurchaseOrderType">
        <xsd:sequence>
            <xsd:element 
                name="shipTo" 
                type="USAddress"/>
            <xsd:element 
                name="billTo" 
                type="USAddress"/>
            <xsd:element 
                ref="comment" 
                minOccurs="0"/>
            <xsd:element 
                name="items"
                type="Items"/>
        </xsd:sequence>
        <xsd:attribute 
            name="orderDate"
            type="xsd:date"/>
    </xsd:complexType>

</xs:schema>
